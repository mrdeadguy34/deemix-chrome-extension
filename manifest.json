{
	"manifest_version": 2,
	"name": "deemix-dl",
	"description": "Download album or song from deezer.",
	"version": "0.1.5",
	"author": "mrdeadguy34",
	"icons": {
		"256": "icons/256.png",
		"128": "icons/128.png",
		"64": "icons/64.png",
		"32": "icons/32.png",
		"16": "icons/16.png"
	},
	"content_scripts": [{
		"css": ["styles.css"],
		"js": ["content.js"],
		"matches": [
			"https://www.deezer.com/us/album/*"
		]
	}]
}